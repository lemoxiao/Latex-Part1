\DeclareNameFormat{prefix-family-given-suffix}{%
  \nameparts{#1}%
  \ifgiveninits
  {\usebibmacro{name:pfgs}
    {\namepartfamily}
    {\namepartgiveni}
    {\namepartprefix}
    {\namepartsuffix}}
  {\usebibmacro{name:pfgs}
    {\namepartfamily}
    {\namepartgiven}
    {\namepartprefix}
    {\namepartsuffix}}%
  \usebibmacro{name:andothers}}




