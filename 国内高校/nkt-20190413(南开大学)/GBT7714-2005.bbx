\newbibmacro*{name:pfgs}[4]{%
  \ifuseprefix
  {\usebibmacro{name:delim}{#3#1}%
    \usebibmacro{name:hook}{#3#1}%
    \blx@xifempty{#3}{}{%
      \ifcapital
      {\mkbibnameprefix{\MakeCapital{#3}}\isdot}
      {\mkbibnameprefix{#3}\isdot}%
      \ifprefchar{}{\bibnamedelimc}}%
    \MakeUppercase{#1}\isdot
    \blx@xifempty{#2}{}{\revsdnamepunct\bibnamedelimd\MakeUppercase{#2}\isdot}%
    \blx@xifempty{#4}{}{\bibnamedelimd\mkbibnameaffix{#4}\isdot}}
   {\usebibmacro{name:delim}{#1}%
     \usebibmacro{name:hook}{#1}%
     \MakeUppercase{#1}\isdot
     \blx@xifempty{#2#3}{}{\revsdnamepunct}%
     \blx@xifempty{#2}{}{\bibnamedelimd\MakeUppercase{#2}\isdot}%
     \blx@xifempty{#3}{}{\bibnamedelimd\mkbibnameprefix{#3}\isdot}%
     \blx@xifempty{#4}{}{,\bibnamedelimd\mkbibnameaffix{#4}\isdot}}}


