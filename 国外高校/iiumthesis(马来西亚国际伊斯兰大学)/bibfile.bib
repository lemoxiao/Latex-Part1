@INPROCEEDINGS{2013-moaaz,
AUTHOR="Moaaz {Elhag Ali} and Khaizuran Abdullah and Mohammad Siddiqi and Ahmad
Fadzil Ismail",
TITLE="{{Impulsive Noise Mitigation in Haar {DWT} Based {OFDM} systems}}",
BOOKTITLE="{2013 IEEE 11th Malaysia International Conference on Communications (MICC
2013) (MICC'13)}",
ADDRESS="{Kuala Lumpur, Malaysia}",
DAYS=26,
MONTH=nov,
YEAR=2013,
KEYWORDS="IN, DWT, BPSK, Bernoulli-Gaussian",
ABSTRACT="In this work, a simple mitigation technique for the effect of impulsive
noise in Haar discrete wavelet transform (DWT) based OFDM systems is
presented. Based on the analysis for a certain configuration of Haar
DWT-OFDM, redundancy in the transmitted samples was found and exploited to
mitigate the impulsive noise by replacing the corrupted samples. Under the
assumption of Bernoulli-Gaussian model for impulsive noise, the proposed
technique is efficient with low probability of impulse occurrence. With p =
0:001 and BPSK scheme, it can achieve BER of 10^-4 with 5 dB gain in SNR.."
}

