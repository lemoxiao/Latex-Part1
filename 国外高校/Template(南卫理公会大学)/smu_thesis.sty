 \addcontentsline{toc}{chapter}{#1}\fi
 \vbox{\vskip 0.56in} %% 2" FROM TOP OF PAGE TO 'CHAPTER'
 \chaptermark{#1}
 %************************NEW CHAPTER DEFINITION*****************
 \if@twocolumn
 \@topnewpage[\@makechapterhead{#2}]
 \else \@makechapterhead{#2}
 \@afterheading\fi
\vskip -0.19in  %% 2 DOUBLE SPACES BETWEEN CHAPTER TITLE AND FIRST SENTENCE
 \chaptertest=0
 \indent
 \everypar={\advance \chaptertest by 1}

 }

 \def\@schapter#1{\if@twocolumn \@topnewpage[\@makeschapterhead{#1}]
 \else \@makeschapterhead{#1}
